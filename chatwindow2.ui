   <property name="styleSheet">
    <string notr="true">/* QSpinBox 기본 스타일 - 파스텔 톤 */
QSpinBox {
    background-color: #fce4ec;  /* 부드러운 핑크색 배경 */
    border: 1px solid #f8bbd0;  /* 연한 핑크 테두리 */
    border-radius: 5px;         /* 테두리 둥글게 */
    padding: 2px;
    color: #6a1b9a;             /* 텍스트 색상 - 부드러운 보라색 */
    font: bold 12px;            /* 텍스트 굵게 */
}

/* QSpinBox의 위/아래 버튼(화살표) 스타일 */
QSpinBox::up-button, QSpinBox::down-button {
    background-color: #f8bbd0;  /* 버튼의 기본 배경색 - 연한 핑크 */
    border: none;               /* 버튼 경계선 제거 */
    width: 16px;                /* 버튼 넓이 설정 */
}

QSpinBox::up-arrow, QSpinBox::down-arrow {
    width: 8px;                 /* 화살표 크기 */
    height: 8px;
    image: url(arrow.png);      /* 화살표 이미지 설정 가능 */
}

/* 마우스를 버튼에 올렸을 때 색상 변화 */
QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background-color: #c5cae9;  /* 마우스 오버 시 연한 파란색으로 변경 */
}

/* QSpinBox의 입력 필드(텍스트 표시 영역) 스타일 */
QSpinBox::editable {
    background-color: #f3e5f5;  /* 입력 필드 배경색 - 연한 보라색 */
    color: #6a1b9a;             /* 텍스트 색상 */
}

/* 스핀박스 버튼 클릭 시의 색상 변화 */
QSpinBox::up-button:pressed, QSpinBox::down-button:pressed {
    background-color: #b39ddb;  /* 클릭 시 진한 보라색으로 변경 */
}
</string>

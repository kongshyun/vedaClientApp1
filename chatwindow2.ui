    padding: 3px;                /* 슬라이더 내부 패딩 */
}
QSlider::groove:horizontal {
    border: 0px solid rgb(255, 255, 255); /* 트랙 경계선 */
    height: 10px;               /* 트랙의 높이 설정 */
    background: rgb(72, 72, 72);     /* 트랙 배경색 */
    margin: 1px 0;
}
QSlider::handle:horizontal {
    background: rgb(255, 255, 127);      /* 핸들의 색상 */
    border: 2px solid #5c5c5c;
    width: 15px;                /* 핸들의 넓이 설정 */
	height:10 px;
    margin: -5px 0;             /* 핸들의 마진 */
    border-radius: 5px;         /* 핸들의 모서리 둥글게 */
}

/* 슬라이더 핸들을 클릭하고 움직일 때의 스타일 */
